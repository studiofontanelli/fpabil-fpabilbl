<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC
 "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
 "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<context id="servsifp" targetRuntime="MyBatis3">
		
		<commentGenerator>
			<!-- whether the removal of the automatically generated annotation true： 
				is ： false: No information -->
			<property name="suppressAllComments" value="true" />
			
		</commentGenerator>
		<!-- database connection ： drive 、 connection address 、 username 、 password 
			, here configuration is mysql ， can of course configuration oracle databases -->
		<jdbcConnection driverClass="org.postgresql.Driver"
			connectionURL="jdbc:postgresql://localhost:5432/fpabil"
			userId="jabi" password="jabiuser">
		</jdbcConnection>
		
		
		<!-- default false， JDBC DECIMAL and NUMERIC type resolution for Integer， 
			true to JDBC DECIMAL and NUMERIC type resolution for java.math.BigDecimal -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- targetProject: generation position PO -->
		<javaModelGenerator  targetPackage="it.csi.fpabil.fpabilbl.integration.dto"  
			targetProject="../fpabilbl-integration/src/main/java">
			
			<!-- enableSubPackages: class of the whether let schema as package suffix -->
			<property name="enableSubPackages" value="false" />
			
			<!-- from a database of return values be cleared before and after spaces -->
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		
		<!-- targetProject:mapper Position -->
		<sqlMapGenerator targetPackage="it.csi.fpabil.fpabilbl.integration.dao" 
				targetProject="../fpabilbl-integration/src/main/resources">
			<!-- enableSubPackages: mapping file generated by whether let schema as 
				position -->
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		<!-- targetPackage：mapper package suffix -->
		
		<javaClientGenerator type="XMLMAPPER"  targetPackage="it.csi.fpabil.fpabilbl.integration.dao"  
				targetProject="../fpabilbl-integration/src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		
		<!-- generate code -->
		
		
		<table tableName="db_iride_stato_richiesta" />
	
		
		<table tableName="tab_doc_identita" >	
		</table>
		
		<table tableName="db_iride_richiesta_operatore" >	
			<generatedKey column="id_richiesta_operatore" sqlStatement="JDBC" identity="true" />
		</table>
		<table tableName="db_iride_richiesta_utente" >	
		</table>
		
		
		

		


	</context>
</generatorConfiguration>