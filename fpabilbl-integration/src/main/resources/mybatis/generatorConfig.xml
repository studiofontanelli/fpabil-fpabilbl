<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC
 "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
 "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<context id="fpabil_fpabilbl" targetRuntime="MyBatis3">

		<plugin
			type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />


		<commentGenerator>
			<!-- whether the removal of the automatically generated annotation true： 
				is ： false: No information -->
			<property name="suppressAllComments" value="true" />

		</commentGenerator>
		<!-- database connection ： drive 、 connection address 、 username 、 password 
			, here configuration is mysql ， can of course configuration oracle databases -->

		<jdbcConnection
			driverClass="org.postgresql.Driver"
			connectionURL="jdbc:oracle:thin:@test10b.csi.it:1521:TEST10B"
			userId="developer" password="mypass">
		</jdbcConnection>


		<!-- default false， JDBC DECIMAL and NUMERIC type resolution for Integer， 
			true to JDBC DECIMAL and NUMERIC type resolution for java.math.BigDecimal -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- targetProject: generation position PO -->
		<javaModelGenerator
			targetPackage="it.csi.stasp.stawapp.integration.dto"
			targetProject="src/main/java">

			<!-- enableSubPackages: class of the whether let schema as package suffix -->
			<property name="enableSubPackages" value="false" />

			<!-- from a database of return values be cleared before and after spaces -->
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- targetProject:mapper Position -->
		<sqlMapGenerator
			targetPackage="it.csi.stasp.stawapp.integration.dao"
			targetProject="src/main/resources">
			<!-- enableSubPackages: mapping file generated by whether let schema as 
				position -->
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		<!-- targetPackage：mapper package suffix -->

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="it.csi.stasp.stawapp.integration.dao"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>


		<!-- generate code -->
		<table tableName="RSCA_D_TIPO_VEICOLO" />
		<table tableName="RSCA_D_TIPO_CANALE" />
		<table tableName="RSCA_D_TIPO_RICHIESTA" />
		<table tableName="RSCA_D_STATO_RICHIESTA" />

		<table tableName="RSCA_T_RICHIESTA_SOGGETTO">
			<generatedKey column="ID_RICHIESTA_SOGGETTO"
				sqlStatement="select seq_rsca_t_richiesta_soggetto.nextval from dual"></generatedKey>
		</table>
		<table tableName="RSCA_T_RICHIESTA_VEICOLO">
			<generatedKey column="ID_RICHIESTA_VEICOLO"
				sqlStatement="select seq_rsca_t_richiesta_veicolo.nextval from dual"></generatedKey>
		</table>

		<table tableName="RSCA_R_SOGGETTO_CANALE" />


		<table tableName="RSCA_T_RICH_COMUNICA_BOLLO" />
		<table tableName="RSCA_T_COMUNICAZIONE">
			<generatedKey column="ID_COMUNICAZIONE"
				sqlStatement="select seq_t_comunicazione.nextval from dual"></generatedKey>
		</table>
		<table tableName="RSCA_T_RICHIESTA_COMUNICAZIONE">
			<generatedKey column="ID_RICHIESTA_COMUNICAZIONE"
				sqlStatement="select seq_t_richiesta_comunicazione.nextval from dual"></generatedKey>
		</table>

	</context>
</generatorConfiguration>